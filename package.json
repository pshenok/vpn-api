{
  "name": "vpn-api",
  "version": "1.0.0",
  "description": "vpn-api",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:dev": "node -r ts-node/register -r dotenv/config src/index.ts",
    "start:w": "nodemon --watch src --ext ts,json --exec 'pnpm start:dev'",
    "clear": "rm -rf dist",
    "test:unit": "jest --config jest.config.json",
    "test:e2e": "jest --runInBand --config jest.config.e2e.json",
    "test:w": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint --quiet --ext ts,js .",
    "lint:fix": "eslint --fix --ext .ts .",
    "core": "pnpm add core@git+git@github.com:layereddd/core.git#main",
    "db:generate": "npx prisma generate",
    "db:validate": "npx prisma validate",
    "db:migrate": "npx prisma migrate dev --name",
    "db:reset": "npx prisma migrate reset --force",
    "db:deploy": "npx prisma migrate deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pshenok/vpn-api"
  },
  "author": "pshenok",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@prisma/client": "3.15.2",
    "@types/cls-hooked": "^4.3.3",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^8.3.4",
    "awilix": "^6.1.0",
    "axios": "^0.27.2",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.0.1",
    "cls-hooked": "^4.2.2",
    "core": "github:layereddd/core#main",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fast-safe-stringify": "^2.1.1",
    "helmet": "^5.1.0",
    "json-web-token": "^3.2.0",
    "multer": "1.4.5-lts.1",
    "prisma": "^3.15.2",
    "prisma-dbml-generator": "^0.9.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/bcrypt": "^3.0.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.20.55",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "dotenv": "^8.6.0",
    "eslint": "^6.8.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.16",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
